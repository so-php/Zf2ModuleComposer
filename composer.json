{
    "name": "so-php/Zf2ModuleComposer",
    "description": "Zf2 Application module that exposes composer package management through a web interface",
    "keywords": [
        "framework",
        "zf2",
        "so-php",
        "composer"
    ],
    "homepage": "https://github.com/so-php",
    "type": "zend-module",
    "autoload": {
        "psr-4": {"Zf2ModuleComposer\\": "src/Zf2ModuleComposer"}
    },    
    "require": {
        "php": ">=5.3.3",
        "zendframework/zftool": "dev-master",
        "composer/composer": "dev-master",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    }
}
